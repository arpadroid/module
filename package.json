{
    "name": "@arpadroid/module",
    "version": "0.0.1",
    "description": "Comprehensive build system and development toolkit for JavaScript/TypeScript projects with unified Rollup, Storybook, Jest, and ESLint configuration.",
    "exports": {
        ".": "./src/index.js",
        "./logger": "./src/utils/terminalLogger.mjs",
        "./storybook/test": {
            "types": "./src/storybook/exports/test.d.ts",
            "import": "./src/storybook/exports/test.js"
        },
        "./storybook/addons": "./src/storybook/index.mjs",
        "./storybook/main": "./src/storybook/main.ui.js",
        "./storybook/preview": "./src/storybook/preview.ui.js",
        "./storybook/middleware": "./.storybook/middleware.js",
        "./storybook/manager": "./.storybook/manager.jsx",
        "./storybook/decorators": "./src/storybook/decorators.js"
    },
    "bin": {
        "arpadroid-build": "./scripts/build-project.mjs",
        "arpadroid-test": "./scripts/test-project.mjs"
    },
    "types": "./dist/@types/types.compiled.d.ts",
    "type": "module",
    "files": [
        "src/",
        "scripts/",
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "build:project": "node ./scripts/build-project.js",
        "test": "node ./scripts/test-project.mjs --storybook --jest --port=6000",
        "install:project": "node ./scripts/install-projects.mjs",
        "stop:storybook:ci": "pm2 stop srv-storybook && pm2 delete srv-storybook",
        "postinstall": "npx playwright install",
        "build": "node ./scripts/build-project.mjs --minify",
        "clean": "rm -rf ./dist ./node_modules package-lock.json",
        "clean:build": "npm run clean && npm install && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/module.git"
    },
    "keywords": [
        "build-system",
        "rollup",
        "storybook",
        "jest",
        "typescript",
        "eslint",
        "babel",
        "bundler",
        "development-tools",
        "testing",
        "ui-components",
        "css-bundling",
        "arpadroid",
        "build-tools",
        "javascript",
        "esm"
    ],
    "author": "Andres Vaquero / @arpadroid",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/module/issues"
    },
    "homepage": "https://github.com/arpadroid/module#readme",
    "dependencies": {
        "@types/node": "^22.10.10"
    },
    "devDependencies": {
