{
    "name": "@arpadroid/module",
    "version": "0.0.1",
    "description": "Comprehensive build system and development toolkit for JavaScript/TypeScript projects with unified Rollup, Storybook, Jest, and ESLint configuration.",
    "exports": {
        ".": "./src/index.js",
        "./storybook/test": {
            "types": "./src/storybook/exports/test.d.ts",
            "import": "./src/storybook/exports/test.js"
        },
        "./storybook/addons": "./src/storybook/index.mjs",
        "./storybook/main": "./src/storybook/main/main.ui.js",
        "./storybook/preset": "./src/storybook/preset.js",
        "./storybook/preview": "./src/storybook/preview/preview.ui.js",
        "./storybook/middleware": "./.storybook/middleware.cjs",
        "./storybook/manager": "./.storybook/manager.jsx",
        "./storybook/decorators": "./src/storybook/preview/decorators.js",
        "./tsconfig": "./tsconfig.json",
        "./vitest/config": "./src/storybook/vitest.config.mjs",
        "./jest/setup": "./src/jest/setupTests.mjs",
        "./jest/config/jsdom": "./src/jest/jest-jsdom.config.mjs",
        "./jest/config/node": "./src/jest/jest-node.config.mjs",
        "./jest/config": "./src/jest/jest-global.config.mjs"
    },
    "bin": {
        "arpadroid-build": "./scripts/build-project.mjs",
        "arpadroid-test": "./scripts/test-project.mjs",
        "arpadroid-install": "./scripts/install-project.mjs",
        "arpadroid-clean": "./scripts/clean-project.mjs",
        "arpadroid-clean-projects": "node ./scripts/clean-projects.mjs"
    },
    "types": "./dist/@types/types.compiled.d.ts",
    "type": "module",
    "files": [
        "src/",
        "scripts/",
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "build:project": "node ./scripts/build-project.js",
        "test": "node ./scripts/test-project.mjs --storybook --jest",
        "test:jest": "node ./scripts/test-project.mjs --jest",
        "t": "npm run test -- --slim",
        "t:w": "npm run test -- --slim --watch",
        "install:project": "node ./scripts/install-projects.mjs",
        "postinstall": "npx playwright install",
        "build": "node ./scripts/build-project.mjs --minify",
        "clean": "node ./scripts/clean-project.mjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arpadroid/module.git"
    },
    "keywords": [
        "build-system",
        "rollup",
        "storybook",
        "jest",
        "typescript",
        "eslint",
        "babel",
        "bundler",
        "development-tools",
        "testing",
        "ui-components",
        "css-bundling",
        "arpadroid",
        "build-tools",
        "javascript",
        "esm"
    ],
    "author": "Andres Vaquero / @arpadroid",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arpadroid/module/issues"
    },
    "homepage": "https://github.com/arpadroid/module#readme",
    "peerDependencies": {
        "@arpadroid/logger": "file:../logger",
        "@arpadroid/style-bun": "file:../style-bun",
        "@arpadroid/tools-iso": "file:../tools-iso",
        "@arpadroid/tools-node": "file:../tools-node"
    },
    "devDependencies": {
        "@arpadroid/i18n": "file:../i18n",
        "@babel/core": "^7.28.6",
        "@babel/eslint-parser": "^7.28.6",
        "@babel/parser": "^7.28.6",
        "@babel/preset-env": "^7.28.6",
        "@babel/types": "^7.28.6",
        "@chromatic-com/storybook": "^5.0.1",
        "@rollup/plugin-alias": "^6.0.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-html": "^2.0.0",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-multi-entry": "^7.1.0",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-replace": "^6.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@storybook/addon-a11y": "^10.2.8",
        "@storybook/addon-docs": "^10.2.8",
        "@storybook/addon-links": "^10.2.8",
        "@storybook/addon-themes": "^10.2.8",
        "@storybook/addon-vitest": "^10.2.8",
        "@storybook/cli": "^10.2.8",
        "@storybook/web-components-vite": "^10.2.8",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.2.2",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.2.3",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitest/browser": "^4.0.18",
        "@vitest/browser-playwright": "^4.0.18",
        "@vitest/coverage-v8": "^4.0.18",
        "babel-jest": "^30.2.0",
        "chalk": "^5.3.0",
        "chalk-animation": "^2.0.3",
        "eslint": "^9.39.2",
        "eslint-plugin-compat": "^6.1.0",
        "eslint-plugin-eslint-plugin": "^7.3.0",
        "eslint-plugin-functional": "^9.0.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsdoc": "^62.5.4",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-unsanitized": "^4.1.4",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.6",
        "eslint-plugin-unicorn": "^62.0.0",
        "eslint-plugin-xss": "^0.1.12",
        "glob": "^13.0.0",
        "@custom-elements-manifest/analyzer": "^0.11.0",
        "globals": "^17.3.0",
        "http-proxy-middleware": "^3.0.5",
        "http-server": "^14.1.1",
        "jest": "^30.2.0",
        "jest-ci-spec-reporter": "^1.0.4",
        "jest-console-group-reporter": "^1.1.1",
